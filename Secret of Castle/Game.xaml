<Window x:Name="Game1" x:Class="Secret_of_Castle.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Secret_of_Castle"
        mc:Ignorable="d"
        Title="Secret Of Castle Game" Height="720" Width="1280"
        WindowState="Maximized" WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode = "NoResize" Icon="/Icon.jpeg" KeyDown="Game1_KeyDown">
    <Grid x:Name="Game_grid">
        <Canvas x:Name="CanvasGame" Focusable="True" KeyUp="kbup" KeyDown="kbdown">
            <Rectangle x:Name="Player" Canvas.Left="590" Canvas.Top="285"  Width="100" Height="150">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="/Player.png"/>
                </Rectangle.Fill>
            </Rectangle>
        </Canvas>
    </Grid>
</Window>
